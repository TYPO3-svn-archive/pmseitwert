
<T3DataStructure>
  <meta>
    <langDisable>1</langDisable>
  </meta>
  <ROOT>
    <type>array</type>
    <el>
      <domain>
        <TCEforms>
          <exclude>1</exclude>
          <label>LLL:EXT:pmseitwert/locallang.xml:pi1_flexform.domain</label>
          <config>
            <type>select</type>
            <itemsProcFunc>tx_pmseitwert_flexform->select_domain</itemsProcFunc>
          </config>
        </TCEforms>
      </domain>
      <templateFile>
        <TCEforms>
          <label>LLL:EXT:pmseitwert/locallang.xml:pi1_flexform.template_file</label>
          <config>
            <type>group</type>
            <internal_type>file</internal_type>
            <allowed>tmpl,html,htm</allowed>
            <size>1</size>
            <maxsize>100</maxsize>
            <uploadfolder>uploads/tx_pmseitwert</uploadfolder>
            <maxitems>1</maxitems>
            <minitems>0</minitems>
            <selectedListStyle>Width:350px</selectedListStyle>
          </config>
        </TCEforms>
      </templateFile>
    </el>
  </ROOT>
</T3DataStructure>